<?xml version="1.0" encoding="UTF-8"?>
<services xmlns="http://www.demandware.com/xml/impex/services/2014-09-26">
    <service-credential service-credential-id="ocapi.auth.cred">
        <url>{{HOST}}//dw/oauth2/access_token</url>
        <user-id/>
        <password encrypted="true" encryption-type="common.export">password</password>
    </service-credential>

    <service-credential service-credential-id="ocapi.internal.cred">
        <url>{{HOST}}//s/-/dw/data/v23_1/</url>
        <user-id/>
        <password encrypted="true" encryption-type="common.export">password</password>
    </service-credential>

    <service-profile service-profile-id="ocapi.auth.prof">
        <timeout-millis>1000</timeout-millis>
        <rate-limit-enabled>false</rate-limit-enabled>
        <rate-limit-calls>0</rate-limit-calls>
        <rate-limit-millis>0</rate-limit-millis>
        <cb-enabled>false</cb-enabled>
        <cb-calls>0</cb-calls>
        <cb-millis>0</cb-millis>
    </service-profile>

    <service-profile service-profile-id="ocapi.internal.prof">
        <timeout-millis>5000</timeout-millis>
        <rate-limit-enabled>false</rate-limit-enabled>
        <rate-limit-calls>0</rate-limit-calls>
        <rate-limit-millis>0</rate-limit-millis>
        <cb-enabled>false</cb-enabled>
        <cb-calls>0</cb-calls>
        <cb-millis>0</cb-millis>
    </service-profile>

    <service service-id="http.ocapi.auth">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>ocapi</log-prefix>
        <comm-log-enabled>false</comm-log-enabled>
        <force-prd-enabled>true</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>ocapi.auth.prof</profile-id>
        <credential-id>ocapi.auth.cred</credential-id>
    </service>

    <service service-id="http.ocapi.internal">
        <service-type>HTTP</service-type>
        <enabled>true</enabled>
        <log-prefix>ocapi</log-prefix>
        <comm-log-enabled>false</comm-log-enabled>
        <force-prd-enabled>true</force-prd-enabled>
        <mock-mode-enabled>false</mock-mode-enabled>
        <profile-id>ocapi.internal.prof</profile-id>
        <credential-id>ocapi.internal.cred</credential-id>
    </service>

</services>
